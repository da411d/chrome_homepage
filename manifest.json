{
"update_url": "https://clients2.google.com/service/update2/crx",

  "version": "0.1.6.4",
  "name": "HomePage by DA411D",
  "description": "A HomePage by David Manzhula @da411d",
  "offline_enabled": true,
  "default_locale" : "en",
  "chrome_url_overrides": {
    "newtab": "/index.html"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "/assets/js/script.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "/assets/images/icon/128.png",
      "16": "/assets/images/icon/16.png",
      "48": "/assets/images/icon/48.png"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com https://www.google.com; object-src https://www.google-analytics.com https://www.google.com 'self'",
  "icons": {
    "128": "/assets/images/icon/128.png",
    "16": "/assets/images/icon/16.png",
    "48": "/assets/images/icon/48.png"
  },
  "permissions": [
    "<all_urls>",
    "http://www.google.com.ua/",
    "tabs",
    "storage",
    "unlimitedStorage",
    "cookies",
    "contextMenus",
	"geolocation", 
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "management",
    "bookmarks",
    "topSites",
    "identity", 
	"identity.email"
  ],
  "web_accessible_resources": [ "*" ],
  "manifest_version": 2
}
